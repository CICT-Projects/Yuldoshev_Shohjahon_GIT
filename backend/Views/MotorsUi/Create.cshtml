@model backend.Models.Motor
@{
  Layout = "~/Views/Shared/_Layout.cshtml";
  ViewData["Title"] = "Create Motor";
}
<h1>Create Motor</h1>
<form method="post" action="/motors-ui/create">
  @Html.AntiForgeryToken()
  <label>Name: <input name="Name" required /></label><br/>
  <label>Brand: <input name="Brand" required /></label><br/>
  <button type="submit">Create</button> <a href="/motors-ui">Cancel</a>
</form>

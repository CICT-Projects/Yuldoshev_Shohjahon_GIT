@model IEnumerable<backend.Models.Shina>
<!doctype html>
<html><head><meta charset="utf-8" /><title>Shina List</title></head>
<body>
  <h1>Shinas</h1>
  <p><a href="/shina-ui/create">Create New</a></p>
  <table>
    <thead><tr><th>ID</th><th>Name</th><th>Brand</th><th>Actions</th></tr></thead>
    <tbody>
    @foreach(var s in Model) {
      <tr>
        <td>@s.Id</td>
        <td>@s.Name</td>
        <td>@s.Brand</td>
        <td><a href="/shina-ui/edit/@s.Id">Edit</a> | <a href="/shina-ui/delete/@s.Id">Delete</a></td>
      </tr>
    }
    </tbody>
  </table>
</body></html>
